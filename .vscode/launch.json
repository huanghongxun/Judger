{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) runguard",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/runguard/bin/runguard",
            "args": [
                "--user=domjudge-run",
                "--group=domjudge-run",
                "--wall-time=1",
                "--out-meta test.meta",
                "--",
                "/tmp/test"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb-sudo",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb and add better child debugging",
                    "text": "-enable-pretty-printing -gdb-set detach-on-fork off",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) test-3.0",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/judge-system",
            "args": [
                "--enable-3=config-moj.json",
                "--cores=4",
                "--auto-workers",
                "--exec-dir=${workspaceFolder}/exec",
                "--cache-dir=/tmp",
                "--run-dir=/tmp",
                "--log-dir=/tmp",
                "--chroot-dir=/chroot",
                "--cache-random-data=100",
                "--run-user=domjudge-run",
                "--run-group=domjudge-run"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/3.0",
            "environment": [
                { "name": "GLOG_alsologtostderr", "value": "1"},
                { "name": "GLOG_colorlogtostderr", "value": "1"}
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb-sudo",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb and add better child debugging",
                    "text": "-enable-pretty-printing -gdb-set detach-on-fork off",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) unit-test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/unit_test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/",
            "environment": [
                { "name": "GLOG_alsologtostderr", "value": "1"},
                { "name": "GLOG_colorlogtostderr", "value": "1"}
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb-sudo",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb and add better child debugging",
                    "text": "-enable-pretty-printing -gdb-set detach-on-fork off",
                    "ignoreFailures": true
                },
                {
                    "description": "Enable printing long string",
                    "text": "set print elements 0",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) test-2.0",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/judge-system",
            "args": [
                "--enable-2=config-mcourse.json",
                "--worker=1",
                "--exec-dir=${workspaceFolder}/exec",
                "--cache-dir=/tmp",
                "--run-dir=/tmp",
                "--log-dir=/tmp",
                "--chroot-dir=/chroot",
                "--cache-random-data=100",
                "--run-user=domjudge-run",
                "--run-group=domjudge-run"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/2.0",
            "environment": [
                { "name": "GLOG_alsologtostderr", "value": "1"},
                { "name": "GLOG_colorlogtostderr", "value": "1"}
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb-sudo",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb and add better child debugging",
                    "text": "-enable-pretty-printing -gdb-set detach-on-fork off",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) matrix6",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/judge-system",
            "args": [
                "--enable-3=/home/matrix6/config-moj.json",
                "--auto-workers",
                "--cores=1"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/",
            "environment": [
                { "name": "GLOG_alsologtostderr", "value": "1"},
                { "name": "GLOG_colorlogtostderr", "value": "1"},
                { "name": "CACHEDIR", "value": "/tmp"},
                { "name": "RUNDIR", "value": "/tmp"},
                { "name": "CHROOTDIR", "value": "/chroot"},
                { "name": "CACHERANDOMDATA", "value": "100"},
                { "name": "RUNUSER", "value": "domjudge-run"},
                { "name": "RUNGROUP", "value": "domjudge-run"},
                { "name": "DEBUG", "value": "1"},
                { "name": "ELASTIC_APM_SERVICE_NAME", "value": "judge-system"},
                { "name": "ELASTIC_APM_SERVER_URL", "value": "http://192.168.1.113:31000"},
                { "name": "ELASTIC_APM_ENVIRONMENT", "value": "test" }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb and add better child debugging",
                    "text": "-enable-pretty-printing -gdb-set detach-on-fork off",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Attach to gdb server",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/bin/judge-system",
            "processId": "${command:pickProcess}",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb and add better child debugging",
                    "text": "-enable-pretty-printing -gdb-set detach-on-fork off",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}